{
  "name": "redux-inputs",
  "version": "2.0.0-rc2",
  "description": "Redux actions and reducers for managing and validating forms and inputs",
  "homepage": "https://github.com/zillow/redux-inputs",
  "repository": {
    "type": "git",
    "url": "https://github.com/zillow/redux-inputs"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "gulp && npm run build-umd",
    "build-umd": "webpack -p lib/index.js lib/redux-inputs.playground.js",
    "test": "node node_modules/mocha/bin/mocha --compilers js:babel-register tests",
    "build-examples": "webpack --config examples/basic/webpack.config.js",
    "watch-examples": "webpack --watch --config examples/basic/webpack.config.js",
    "serve-examples": "browser-sync start --server --port 3001 --files examples/basic/out.js --startPath /examples/basic/"
  },
  "keywords": [
    "react",
    "redux",
    "form",
    "inputs",
    "validation",
    "submit"
  ],
  "contributors": [
    "Jake Pusateri <jacobp@zillow.com>",
    "Steven Bierstiker <stevenb@zillow.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.5",
    "invariant": "^2.2.0",
    "lodash": "^4.5.1",
    "react": "^15.3.1",
    "react-display-name": "^0.2.0",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^3.5.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-react": "^6.2.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-eslint": "^3.0.1",
    "mocha": "^2.3.4",
    "react": "^15.0.0",
    "redux": "^3.0.0",
    "redux-mock-store": "^1.2.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1"
  },
  "peerDependencies": {
    "redux": "^3.0.0",
    "redux-thunk": "^2.0.0"
  },
  "optionalDependencies": {
    "react": "^0.14.0 || ^15.0.0-0"
  }
}
